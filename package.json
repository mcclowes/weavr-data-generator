{
  "name": "weavr-data-generator",
  "version": "1.0.0",
  "description": "Generate Vague schemas and JSON fixtures from Weavr OpenAPI specifications",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "weavr-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "weavr",
    "vague",
    "openapi",
    "fixtures",
    "test-data"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0"
  }
}
